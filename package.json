{
  "name": "@gip-recia/didacticiel-ent",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "yarn@4.9.4",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GIP-RECIA/Didacticiel-ENT"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "sass --style=compressed --error-css src/assets/scss/:src/assets/css/ && vite build",
    "preview": "vite preview",
    "postinstall": "husky",
    "release": "yarn run -T commit-and-tag-version -t 'didacticiel-ent/v' --path . --release-as $1 || echo 'Please specify a version type (yarn run release -- [patch, minor, major])'",
    "sass-dev": "sass --watch --update src/assets/scss/:src/assets/css/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/query-selector-shadow-dom": "^1.0.4",
    "@uportal/open-id-connect": "^1.40.2",
    "commit-and-tag-version": "^12.5.1",
    "driver.js": "^1.3.6",
    "eslint": "^9.27.0",
    "eslint-plugin-format": "^1.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "query-selector-shadow-dom": "^1.0.1",
    "regenerator-runtime": "^0.14.1",
    "sass": "^1.89.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  }
}
